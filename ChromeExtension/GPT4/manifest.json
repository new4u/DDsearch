
{
    "name": "ViSearch",
    "description": "Beyond the ChatGPT/AI with eyes",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
      "16": "/images/icon-16x16.png"

    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "/images/icon-16x16.png"
      }
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
      "*://*.google.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
                  